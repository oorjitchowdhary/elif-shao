{
  "name": "elif",
  "displayName": "ELIF",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/oorjitchowdhary/elif-shao"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "elif.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "elif.changeTheme",
        "title": "Change Theme"
      },
      {
        "command": "elif.githubAuth",
        "title": "Github Auth"
      },
      {
        "command": "elif.createGitHubIssue",
        "title": "Create GitHub Issue"
      },
      {
        "command": "elif.githubIssueComments",
        "title": "Fetch GitHub Issue Comments"
      },
      {
        "command": "elif.remind",
        "title": "Set Reminder"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "suncalc": "^1.8.0"
    "@types/date-fns": "^2.6.0",
    "date-fns": "^2.23.0",
    "sherlockjs": "^1.4.2",
    "axios": "^0.21.1",
    "polka": "^0.5.2"
  }
}
